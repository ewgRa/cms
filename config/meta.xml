<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE meta []>
<meta 
	license="http://www.opensource.org/licenses/bsd-license.php BSD"
	author="Evgeniy Sokolov &lt;ewgraf@gmail.com&gt;"
	DAExtends="DatabaseRequester"
	namespace="ewgraCms"
>
	<Language>
		<properties>
			<id />
			<abbr />
		</properties>
	</Language>
	
	<Controller>
		<properties>
			<id />
			<name />
			<settings type="array" nullable="true" />
		</properties>
	</Controller>
	
	<PageStatus type="Enumeration" />
		
	<Layout>
		<properties>
			<id />
			<viewFile class="ViewFile" />
		</properties>
	</Layout>
	
	<Page>
		<properties>
			<id />
			<path />
			<preg type="boolean" nullable="true" />
			<layout class="Layout" />
			<status class="PageStatus" />
			<modified />
		</properties>
	</Page>
	
	<PageData>
		<properties>
			<id />
			<page class="Page" />
			<language class="Language" />
			<title nullable="true" />
			<description nullable="true" />
			<keywords nullable="true" />
		</properties>
	</PageData>
	
	<PageController>
		<properties>
			<id />
			<page class="Page" />
			<controller class="Controller" />
			<section nullable="true" />
			<position nullable="true" />
			<priority nullable="true" />
			<settings type="array" nullable="true" />
			<viewFile class="ViewFile" nullable="true" />
		</properties>
	</PageController>
	
	<Site>
		<properties>
			<id />
			<alias />
		</properties>
	</Site>
	
	<ContentType namespace="\ewgraFramework" type="Enumeration" />
	
	<FileSource>
		<properties>
			<id />
			<alias />
		</properties>
	</FileSource>

	<ViewFile>
		<properties>
			<id />
			<contentType class="\ewgraFramework\ContentType" />
			<path />
			<joinable type="boolean" nullable="true" />
			<source class="FileSource" />
		</properties>
	</ViewFile>
</meta>