<?xml version="1.0" encoding="UTF-8"?>
<meta 
	license="http://www.opensource.org/licenses/bsd-license.php BSD"
	author="Evgeniy Sokolov &lt;ewgraf@gmail.com&gt;"
	DAExtends="CmsDatabaseRequester"
>
	<Category>
		<id />
		<alias />
	</Category>
	
	<Content>
		<id />
		<status type="ContentStatus" />
	</Content>
	
	<ContentData>
		<content type="Content" relation="OneToOne" id="true" />
		<language type="Language" relation="OneToOne" id="true" />
		<text />
	</ContentData>
	
	<DebugData>
		<id />
		<session null="true" />
		<data type="array" />
		<date />
	</DebugData>
	
	<Language>
		<id />
		<abbr />
	</Language>
	
	<Layout>
		<id />
		<viewFile type="ViewFile" relation="OneToOne" />
	</Layout>
	
	<Module>
		<id />
		<name />
		<settings type="array" null="true" />
	</Module>
	
	<Navigation>
		<id />
		<category type="Category" relation="OneToOne" />
		<uri />
	</Navigation>
	
	<NavigationData>
		<navigation type="Navigation" relation="OneToOne" id="true" />
		<language type="Language" relation="OneToOne" id="true" />
		<text />
	</NavigationData>
	
	<Page>
		<id />
		<path />
		<preg type="boolean" null="true" />
		<layout type="Layout" relation="OneToOne" />
		<status type="PageStatus" />
		<modified />
	</Page>
	
	<PageData>
		<page type="Page" relation="OneToOne" id="true" />
		<language type="Language" relation="OneToOne" id="true" />
		<title null="true" />
		<description null="true" />
		<keywords null="true" />
	</PageData>
	
	<PageModule>
		<page type="Page" relation="OneToOne" id="true" />
		<module type="Module" relation="OneToOne" id="true" />
		<section null="true" id="true" />
		<position null="true" id="true" />
		<priority null="true" />
		<settings type="array" null="true" />
		<viewFile type="ViewFile" relation="OneToOne" null="true" />
	</PageModule>
	
	<PageRight>
		<page type="Page" relation="OneToOne" id="true" />
		<right type="Right" relation="OneToOne" id="true" />
		<redirectPage type="Page" relation="OneToOne" />
	</PageRight>
	
	<Right>
		<id />
		<alias />
		<name null="true" />
		<role type="boolean" null="true" />
	</Right>
	
	<Site>
		<id />
		<alias />
	</Site>
	
	<User>
		<id />
		<login />
		<password />
	</User>
	
	<UserRight>
		<user type="User" relation="OneToOne" id="true" />
		<right type="Right" relation="OneToOne" id="true" />
	</UserRight>
	
	<ViewFile>
		<id />
		<contentType type="ContentType" />
		<path replaceVariables="true"/>
		<joinable type="boolean" null="true" />
	</ViewFile>
</meta>