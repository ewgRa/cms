<?xml version="1.0" encoding="UTF-8"?>
<meta 
	license="http://www.opensource.org/licenses/bsd-license.php BSD"
	author="Evgeniy Sokolov &lt;ewgraf@gmail.com&gt;"
	DAExtends="CmsDatabaseRequester"
>
	<Category extends="IdentifierObject">
		<properties>
			<id />
			<alias />
		</properties>
	</Category>
	
	<Content extends="IdentifierObject">
		<properties>
			<id />
			<status type="ContentStatus" />
		</properties>
	</Content>
	
	<ContentData>
		<properties>
			<content type="Content" relation="OneToOne" />
			<language type="Language" relation="OneToOne" />
			<text />
		</properties>
	</ContentData>
	
	<DebugData>
		<properties>
			<id />
			<session null="true" />
			<data type="array" />
			<date />
		</properties>
	</DebugData>
	
	<Language extends="IdentifierObject">
		<properties>
			<id />
			<abbr />
		</properties>
	</Language>
	
	<Layout extends="IdentifierObject">
		<properties>
			<id />
			<viewFile type="ViewFile" relation="OneToOne" />
		</properties>
	</Layout>
	
	<Module extends="IdentifierObject">
		<properties>
			<id />
			<name />
			<settings type="array" null="true" />
		</properties>
	</Module>
	
	<Navigation extends="IdentifierObject">
		<properties>
			<id />
			<category type="Category" relation="OneToOne" />
			<uri type="HttpUrl" createFunction="createFromString"/>
		</properties>
	</Navigation>
	
	<NavigationData>
		<properties>
			<navigation type="Navigation" relation="OneToOne" />
			<language type="Language" relation="OneToOne" />
			<text />
		</properties>
	</NavigationData>
	
	<Page extends="IdentifierObject">
		<properties>
			<id />
			<path />
			<preg type="boolean" null="true" />
			<layout type="Layout" relation="OneToOne" />
			<status type="PageStatus" />
			<modified />
		</properties>
	</Page>
	
	<PageData>
		<properties>
			<page type="Page" relation="OneToOne" />
			<language type="Language" relation="OneToOne" />
			<title null="true" />
			<description null="true" />
			<keywords null="true" />
		</properties>
	</PageData>
	
	<PageModule>
		<properties>
			<page type="Page" relation="OneToOne" />
			<module type="Module" relation="OneToOne" />
			<section null="true" />
			<position null="true" />
			<priority null="true" />
			<settings type="array" null="true" />
			<viewFile type="ViewFile" relation="OneToOne" null="true" />
		</properties>
	</PageModule>
	
	<PageRight>
		<properties>
			<page type="Page" relation="OneToOne" />
			<right type="Right" relation="OneToOne" />
			<redirectPage type="Page" relation="OneToOne" />
		</properties>
	</PageRight>
	
	<Right extends="IdentifierObject">
		<properties>
			<id />
			<alias />
			<name null="true" />
			<role type="boolean" null="true" />
		</properties>
	</Right>
	
	<Site extends="IdentifierObject">
		<properties>
			<id />
			<alias />
		</properties>
	</Site>
	
	<User extends="IdentifierObject">
		<properties>
			<id />
			<login />
			<password />
		</properties>
	</User>
	
	<UserRight>
		<properties>
			<user type="User" relation="OneToOne" />
			<right type="Right" relation="OneToOne" />
		</properties>
	</UserRight>
	
	<ViewFile extends="IdentifierObject">
		<properties>
			<id />
			<contentType type="ContentType" />
			<path replaceVariables="true"/>
			<joinable type="boolean" null="true" />
		</properties>
	</ViewFile>
</meta>