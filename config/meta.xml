<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE meta []>
<meta 
	license="http://www.opensource.org/licenses/bsd-license.php BSD"
	author="Evgeniy Sokolov &lt;ewgraf@gmail.com&gt;"
	DAExtends="CmsDatabaseRequester"
>
	<Category>
		<properties>
			<id />
			<alias />
		</properties>
	</Category>
	
	<Content>
		<properties>
			<id />
			<status class="ContentStatus" />
		</properties>
	</Content>
	
	<ContentData>
		<properties>
			<id />
			<content class="Content" />
			<language class="Language" />
			<text />
		</properties>
	</ContentData>
	
	<DebugData>
		<properties>
			<id />
			<session nullable="true" />
			<data type="array" />
			<date />
		</properties>
	</DebugData>
	
	<Language>
		<properties>
			<id />
			<abbr />
		</properties>
	</Language>
	
	<Layout>
		<properties>
			<id />
			<viewFile class="ViewFile" />
		</properties>
	</Layout>
	
	<Module>
		<properties>
			<id />
			<name />
			<settings type="array" nullable="true" />
		</properties>
	</Module>
	
	<Navigation>
		<properties>
			<id />
			<category class="Category" />
			<uri class="HttpUrl" createFunction="createFromString"/>
		</properties>
	</Navigation>
	
	<NavigationData>
		<properties>
			<id />
			<navigation class="Navigation" />
			<language class="Language" />
			<text />
		</properties>
	</NavigationData>
	
	<Page>
		<properties>
			<id />
			<path />
			<preg type="boolean" nullable="true" />
			<layout class="Layout" />
			<status class="PageStatus" />
			<modified />
		</properties>
	</Page>
	
	<PageData>
		<properties>
			<id />
			<page class="Page" />
			<language class="Language" />
			<title nullable="true" />
			<description nullable="true" />
			<keywords nullable="true" />
		</properties>
	</PageData>
	
	<PageModule>
		<properties>
			<id />
			<page class="Page" />
			<module class="Module" />
			<section nullable="true" />
			<position nullable="true" />
			<priority nullable="true" />
			<settings type="array" nullable="true" />
			<viewFile class="ViewFile" nullable="true" />
		</properties>
	</PageModule>
	
	<PageRight>
		<properties>
			<id />
			<page class="Page" />
			<right clss="Right" />
			<redirectPage class="Page" />
		</properties>
	</PageRight>
	
	<Right>
		<properties>
			<id />
			<alias />
			<name nullable="true" />
			<role type="boolean" nullable="true" />
		</properties>
	</Right>
	
	<Site>
		<properties>
			<id />
			<alias />
		</properties>
	</Site>
	
	<User>
		<properties>
			<id />
			<login />
			<password />
		</properties>
	</User>
	
	<UserRight>
		<properties>
			<id />
			<user class="User" />
			<right class="Right" />
		</properties>
	</UserRight>
	
	<ViewFile>
		<properties>
			<id />
			<contentType class="ContentType" />
			<path />
			<joinable type="boolean" nullable="true" />
			<source class="FileSource" />
		</properties>
	</ViewFile>

	<FileSource>
		<properties>
			<id />
			<alias />
		</properties>
	</FileSource>

</meta>